{
  "name": "@hushkey/remq",
  "version": "0.10.0",
  "description": "A simple and efficient job queue system for Deno. Built on Redis, it offers high-performance task scheduling and processing with Deno's modern runtime.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mirairoad/remq"
  },
  "exports": "./mod.ts",
  "imports": {
    "cron-schedule": "jsr:@p4sca1/cron-schedule@^6.0.0",
    "ioredis": "npm:ioredis@^5.9.3"
  },
  "publish": {
    "exclude": [
      ".github/",
      ".vscode/",
      ".git/",
      "**/.DS_Store",
      "Makefile",
      "tests/"
    ]
  },
  "fmt": {
    "useTabs": false,
    "lineWidth": 80,
    "indentWidth": 2,
    "singleQuote": true,
    "proseWrap": "preserve",
    "semiColons": true
  },
  "lint": {
    "include": [
      "core/"
    ],
    "exclude": [
      "examples/",
      "core/",
      "www/"
    ],
    "rules": {
      "exclude": [
        "no-unused-vars",
        "no-explicit-any"
      ],
      "tags": [
        "recommended"
      ]
    }
  }
}